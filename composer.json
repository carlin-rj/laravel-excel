{
    "name": "mckue/laravel-excel",
    "license": "MIT",
    "description": "Laravel-Excel is based on Spartner NL/Lavel Excel code and switches to an xlswriter extension. If your project is using SparnerNL/Lavel Excel and there are big data export performance issues, and you don't want to modify a lot of code, then the current package may be very suitable for you.",
    "keywords": [
        "laravel",
        "php",
        "xlswriter",
        "php-ext-xlswriter",
        "phpexcel",
        "excel",
        "csv",
        "export",
        "import",
        "batch"
    ],
    "autoload": {
        "psr-4": {
            "Mckue\\Excel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mckue\\Excel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wanghaojie",
            "email": "814425737@qq.com"
        }
    ],
    "require": {
        "ext-xlswriter": "*",
        "ext-json": "*",
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "composer/semver": "^3.3",
        "phpoffice/phpspreadsheet": "^1.18"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^10.3",
        "laravel/pint": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mckue\\Excel\\ExcelServiceProvider"
            ]
        }
    }
}
